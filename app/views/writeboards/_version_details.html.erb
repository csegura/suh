<h1>Versions</h1>

<% @writeboard.versions.each do |version| %>
  <%= writeboard_content_for_version version, @current_version_number do %>
    <div class="version">
      <%= writeboard_link_to_version version, @current_version_number %>
      <span class="user">
        <%= nice_access version.access %>
      </span>
      <br/>
      <span class="date">
        <%= nice_date version.updated_at %>
      </span>
      <%=  puts version.inspect %>
    </div>
  <% end %>
<% end %>