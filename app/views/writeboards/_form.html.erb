<p>
</p>
<%= simple_form_for @writeboard do |f| %>
    <%= f.error_notification %>

    <div class="inputs">

      <%= select_permissions f %>

      <%= title 1, t('label.title') %>
      <%= f.input_field :title, :class => :title %>
      <%= title 1, t('label.description') %>
      <%= f.input_field :body, :class => :editor_full, :rows => 40 %>

      <%#= render "application/sub_documents", :f => f %>

      <%= submit_or_cancel f, :back, :class => :full %>
    </div>
<% end %>