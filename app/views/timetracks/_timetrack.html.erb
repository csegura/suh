<%= content_tag_for :tr, timetrack, :class => timetrack.in_trash? ? "in_trash" : "" do -%>
    <td class="what">
      <%= timetrack_hours timetrack %>
    </td>
    <td class="item">
      <%= timetrack_link timetrack %>
    </td>
    <td class="date">
      <%= nice_date timetrack.tracked_at, :short %>
    </td>
<% end -%>
